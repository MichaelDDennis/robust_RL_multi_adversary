<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="true"/>
    <arg name="ns" default=""/>
    <arg name="rl_params" default="rl_params_img_dyn"/>
    <!-- <arg name="rl_params" default="rl_params_scan"/> -->

    <arg name="update_rate" default="10000"/>
    <!-- <arg name="update_rate" default="300"/> -->

	<arg name="map_path" default="home/ubuntu/map/turltebot_map.yaml"/>

    <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
    <arg name="multi_robot_name" default=""/>

    <include file="$(find turtlebot3_bringup)/launch/includes/description.launch.xml">
        <arg name="model" value="$(arg model)" />
    </include>

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
        <param name="publish_frequency" type="double" value="50.0" />
        <param name="tf_prefix" value="$(arg multi_robot_name)"/>
    </node>

    <node pkg="rl_agent" type="rollout_turtlebot.py" name="rollout_turtlebot.py" output="screen" args="POLICY CHECKPOINT" />

</launch>